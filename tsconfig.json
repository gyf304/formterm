{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "jsx": "react-jsx",
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "formterm": ["."],
      "formterm/*": ["./dist/lib/*"]
    }
  },
}
